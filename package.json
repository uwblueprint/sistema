    "@prisma/client": "^5.17.0",
    "dotenv": "^16.4.5",
    "framer-motion": "^6.3.0",
    "next": "^14.2.13",
    "next-auth": "^5.0.0-beta.19",
    "nodemailer": "^6.9.14",
    "pnpm": "^9.4.0",
    "postgres": "^3.4.4",
    "prisma": "^5.16.0",
