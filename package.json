    "tsx": "^4.19.2"
  },
  "@snaplet/seed": {
    "config": "prisma/seed/seed.config.ts"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed/seed.ts",
    "schema": "./prisma/schema.prisma"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run format"
    ],
    "*.json": [
      "prettier --write"
    ],
    "schema.prisma": [
      "npx prisma format"
    ]
  }
